{
  "$schema": "https://json.schemastore.org/swcrc",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": false,
      "dynamicImport": true
    },
    "target": "es2022",
    "transform": {
      "legacyDecorator": false,
      "decoratorMetadata": false
    },
    "experimental": {
      "plugins": [
        [
          "@runners/swc-plugin",
          {}
        ]
      ]
    }
  },
  "module": {
    "type": "es6",
    "strict": false,
    "strictMode": true,
    "lazy": false,
    "noInterop": false
  }
}

